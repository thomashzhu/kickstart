{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,  
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      "function-paren-newline": ["error", "consistent"],
      "no-trailing-spaces": ["error", {"skipBlankLines": true}],
      "no-use-before-define": ["error", { "variables": false }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "strict": ["error", "global"]
  }
}